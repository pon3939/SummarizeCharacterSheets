{
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "invokeTarget": {
        "target": "template",
        "logicalId": "StockCheckerFunction",
        "templatePath": "${workspaceFolder}/template.yaml"
      },
      "lambda": {
        "runtime": "python3.9"
      },
      "sam": {
        "containerBuild": false,
        "skipNewImageCheck": false,
        "localArguments": ["-e", "${workspaceFolder}/debug_input.json"]
      },
      "name": "StockChecker",
      "preLaunchTask": "install libraries"
    }
  ]
}
